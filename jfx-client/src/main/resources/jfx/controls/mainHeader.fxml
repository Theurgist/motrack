<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="13.0" prefWidth="663.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cc.theurgist.motrack.ui.gui.controllers.FxMainHeaderController">
   <children>
      <HBox maxHeight="1.7976931348623157E308" prefHeight="50.0" prefWidth="263.0" AnchorPane.leftAnchor="0.0">
         <children>
            <Label alignment="CENTER" maxHeight="1.7976931348623157E308" prefHeight="49.0" prefWidth="116.0" text="Общий баланс" />
            <Label fx:id="totalBalance" maxHeight="1.7976931348623157E308" prefHeight="36.0" prefWidth="141.0" text="Nope">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox spacing="5.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Label fx:id="username" alignment="CENTER" maxHeight="1.7976931348623157E308" prefHeight="26.0" prefWidth="157.0" text="Username">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Circle fx:id="avatar" fill="#abebd7" radius="23.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="2.0" />
            <ImageView fx:id="logoffImg" fitHeight="28.0" fitWidth="23.0" onMouseClicked="#doLogoff" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../img/exit.png" />
               </image>
            </ImageView>
         </children>
         <padding>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
